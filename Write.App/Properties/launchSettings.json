{
  "profiles": {
    "Mutation": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "workingDirectory": "$(ProjectDir)",
      "commandLineArgs": "stryker -o -f ../Tests/stryker-config.json -p $(ProjectName) -tp ../Tests/$(ProjectName).Tests/$(ProjectName).Tests.csproj"
    }
  }
}